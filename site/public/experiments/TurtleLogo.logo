to go
    clean
    setpensize 30
    loadpic "/beach.png
    wait 10
    rt 180
    fd 255
end

to left-box
	repeat 4 [ fd 100 lt 90 ]
end

to box-stack
	repeat 2 [box fd 100]
end

to box-car
	box
	left-box
end

to init-migration
   clean 
   loadpic "/migratoryroute.png
   wait 10
end

to egg
    arc 10 360
end

