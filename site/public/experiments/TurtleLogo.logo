to sea
    setcolor 55
    repeat 300
    [setxy random2 "-55vw "55vw
    random2 "-55vh "-35vh
    seth random2 40 50
    setpensize random2 5 10
    setshade random2 50 90
    arc 90 100]
    penup
    setpensize 5
    setshade 50
    setxy 0 0
    seth 0
    pendown
end

to land
    setcolor 12
    repeat 600
    [setxy random2 "-55vw "55vw
    random2 "-35vh "50vh
    setpensize random2 20 25
    setshade random2 90 100
    fd 0]
    penup
    setpensize 5
    setshade 50
    setxy 0 0
    seth 0
    pendown
end

